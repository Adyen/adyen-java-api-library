<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:include schemaLocation="EpasCMS_edit.xsd" />
	<xs:include schemaLocation="EpasCommonTypes.xsd" />

	<!-- -->
	<!-- 1. Messages Definition (PDU)-->
	<!-- -->

	<xs:element name="SaleToPOIRequest">
		<xs:annotation>
			<xs:documentation>Definition: Request message of the EPAS Sale To POI protocol.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="MessageHeader" type="MessageHeader">
					<xs:annotation>
						<xs:documentation>Message header of the Sale to POI protocol message.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:choice>
					<xs:element name="AbortRequest" type="AbortRequest">
						<xs:annotation>
							<xs:documentation>Body of the Abort Request message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BalanceInquiryRequest" type="BalanceInquiryRequest">
						<xs:annotation>
							<xs:documentation>Content of the Balance Inquiry Request message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BatchRequest" type="BatchRequest">
						<xs:annotation>
							<xs:documentation>Content of the Batch Request message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CardAcquisitionRequest" type="CardAcquisitionRequest">
						<xs:annotation>
							<xs:documentation>Content of the Card Acquisition Request message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AdminRequest" type="AdminRequest">
						<xs:annotation>
							<xs:documentation>Content of the Custom Admin Request message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DiagnosisRequest" type="DiagnosisRequest">
						<xs:annotation>
							<xs:documentation>Content of the Diagnosis Request message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DisplayRequest" type="DisplayRequest">
						<xs:annotation>
							<xs:documentation>Content of the Display Request message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EnableServiceRequest" type="EnableServiceRequest">
						<xs:annotation>
							<xs:documentation>Content of the Enable Service Request message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EventNotification" type="EventNotification">
						<xs:annotation>
							<xs:documentation>Content of the EventNotification message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GetTotalsRequest" type="GetTotalsRequest">
						<xs:annotation>
							<xs:documentation>Content of the Get Totals Request message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="InputRequest" type="InputRequest">
						<xs:annotation>
							<xs:documentation>Content of the Input Request message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="InputUpdate" type="InputUpdate">
						<xs:annotation>
							<xs:documentation>Content of the Input Update message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="LoginRequest" type="LoginRequest">
						<xs:annotation>
							<xs:documentation>Content of the Login Request message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="LogoutRequest" type="LogoutRequest">
						<xs:annotation>
							<xs:documentation>Content of the Logout Request message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="LoyaltyRequest" type="LoyaltyRequest">
						<xs:annotation>
							<xs:documentation>Content of the Loyalty Request message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PaymentRequest" type="PaymentRequest">
						<xs:annotation>
							<xs:documentation>Content of the Payment Request message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PINRequest" type="PINRequest">
						<xs:annotation>
							<xs:documentation>Content of the PIN Request message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PrintRequest" type="PrintRequest">
						<xs:annotation>
							<xs:documentation>Content of the Print Request message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CardReaderInitRequest" type="CardReaderInitRequest">
						<xs:annotation>
							<xs:documentation>Content of the Card Reader Init Request message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CardReaderAPDURequest" type="CardReaderAPDURequest">
						<xs:annotation>
							<xs:documentation>Content of the Card Reader APDU Request message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CardReaderPowerOffRequest" type="CardReaderPowerOffRequest">
						<xs:annotation>
							<xs:documentation>Content of the Card Reader Power-Off Request message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ReconciliationRequest" type="ReconciliationRequest">
						<xs:annotation>
							<xs:documentation>Content of the Reconciliation Request message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ReversalRequest" type="ReversalRequest">
						<xs:annotation>
							<xs:documentation>Content of the Reversal Request message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SoundRequest" type="SoundRequest">
						<xs:annotation>
							<xs:documentation>Content of the Sound Request message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="StoredValueRequest" type="StoredValueRequest">
						<xs:annotation>
							<xs:documentation>Content of the Stored Value Request message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TransactionStatusRequest" type="TransactionStatusRequest">
						<xs:annotation>
							<xs:documentation>Content of the TransactionStatus Request message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TransmitRequest" type="TransmitRequest">
						<xs:annotation>
							<xs:documentation>Content of the Transmit Request message.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
				<xs:element name="SecurityTrailer" type="ContentInformation" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Protection of the whole message</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="SaleToPOIResponse">
		<xs:annotation>
			<xs:documentation>Definition: Response message of the EPAS Sale To POI protocol.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="MessageHeader" type="MessageHeader">
					<xs:annotation>
						<xs:documentation>Message header of the Sale to POI protocol message.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:choice>
					<xs:element name="BalanceInquiryResponse" type="BalanceInquiryResponse">
						<xs:annotation>
							<xs:documentation>Content of the Balance Inquiry Response message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BatchResponse" type="BatchResponse">
						<xs:annotation>
							<xs:documentation>Content of the Batch Response message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CardAcquisitionResponse" type="CardAcquisitionResponse">
						<xs:annotation>
							<xs:documentation>Content of the Card Acquisition Response message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AdminResponse" type="AdminResponse">
						<xs:annotation>
							<xs:documentation>Content of the Custom Admin Response message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DiagnosisResponse" type="DiagnosisResponse">
						<xs:annotation>
							<xs:documentation>Content of the Diagnosis Response message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DisplayResponse" type="DisplayResponse">
						<xs:annotation>
							<xs:documentation>Content of the Display Response message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EnableServiceResponse" type="EnableServiceResponse">
						<xs:annotation>
							<xs:documentation>Content of the Enable Service Response message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GetTotalsResponse" type="GetTotalsResponse">
						<xs:annotation>
							<xs:documentation>Content of the Reconciliation Response message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="InputResponse" type="InputResponse">
						<xs:annotation>
							<xs:documentation>Content of the Input Response message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="LoginResponse" type="LoginResponse">
						<xs:annotation>
							<xs:documentation>Content of the Login Response message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="LogoutResponse" type="LogoutResponse">
						<xs:annotation>
							<xs:documentation>Content of the Logout Response message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="LoyaltyResponse" type="LoyaltyResponse">
						<xs:annotation>
							<xs:documentation>Content of the Loyalty Response message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PaymentResponse" type="PaymentResponse">
						<xs:annotation>
							<xs:documentation>Content of the Payment Response message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PINResponse" type="PINResponse">
						<xs:annotation>
							<xs:documentation>Content of the PIN Response message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PrintResponse" type="PrintResponse">
						<xs:annotation>
							<xs:documentation>Content of the Print Response message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CardReaderInitResponse" type="CardReaderInitResponse">
						<xs:annotation>
							<xs:documentation>Content of the Card Reader Init Response message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CardReaderAPDUResponse" type="CardReaderAPDUResponse">
						<xs:annotation>
							<xs:documentation>Content of the Card Reader APDU Response message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CardReaderPowerOffResponse" type="CardReaderPowerOffResponse">
						<xs:annotation>
							<xs:documentation>Content of the Card Reader Power-Off Response message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ReconciliationResponse" type="ReconciliationResponse">
						<xs:annotation>
							<xs:documentation>Content of the Reconciliation Response message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ReversalResponse" type="ReversalResponse">
						<xs:annotation>
							<xs:documentation>Content of the Reversal Response message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SoundResponse" type="SoundResponse">
						<xs:annotation>
							<xs:documentation>Content of the Sound Response message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="StoredValueResponse" type="StoredValueResponse">
						<xs:annotation>
							<xs:documentation>Content of the Stored Value Response message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TransactionStatusResponse" type="TransactionStatusResponse">
						<xs:annotation>
							<xs:documentation>Content of the TransactionStatus Response message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TransmitResponse" type="TransmitResponse">
						<xs:annotation>
							<xs:documentation>Content of the Transmit Response message.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
				<xs:element name="SecurityTrailer" type="ContentInformation" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Protection of the whole message</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<!-- -->
	<!-- 2. Message Components Definition-->
	<!-- -->

	<xs:simpleType name="AbortReason">
		<xs:annotation>
			<xs:documentation>Definition: Reason of aborting a transaction -- Usage: Free text to log for further examination.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="TextString">
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="AbortRequest">
		<xs:annotation>
			<xs:documentation>Definition: Body of the Abort Request message. -- Usage: It conveys Information requested for identification of the message request carrying the transaction to abort. A message to display on the CustomerError Device could be sent by the Sale System (DisplayOutput).</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="MessageReference" type="MessageReference">
				<xs:annotation>
					<xs:documentation>Identification of a previous POI transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AbortReason" type="AbortReason">
				<xs:annotation>
					<xs:documentation>Reason of aborting a transaction</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DisplayOutput" type="DisplayOutput" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Information to display and the way to process the display. --Rule: To display an abort message to the Customer</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="AccessedBy">
		<xs:annotation>
			<xs:documentation>Definition: Identification of an entity accessing data to perform an operation. -- Usage: Allow the synchronisation of customer order processing, when several parts of the Sale System access a customer order at the same time. The Sale entity active on the customer order is identified by this data.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="TextString">
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="AccountNumber">
		<xs:annotation>
			<xs:documentation>Definition: Identification of the customer account.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="TextString">
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="AccountTypeCodeZ">
		<xs:union memberTypes="AccountType TypeCodeExtension"/>
	</xs:simpleType>
	<xs:simpleType name="AccountType">
		<xs:annotation>
			<xs:documentation>Definition: Type of cardholder account used for the transaction -- Usage: Allows a cardholder to select the type of account used for the transaction</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Default">
				<xs:annotation>
					<xs:documentation>Default account</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Savings">
				<xs:annotation>
					<xs:documentation>Savings account</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Checking">
				<xs:annotation>
					<xs:documentation>Checking account</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CreditCard">
				<xs:annotation>
					<xs:documentation>Credit card account</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Universal">
				<xs:annotation>
					<xs:documentation>Universal account</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Investment">
				<xs:annotation>
					<xs:documentation>Investment account</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CardTotals">
				<xs:annotation>
					<xs:documentation>Card totals</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EpurseCard">
				<xs:annotation>
					<xs:documentation>e-Purse card account</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="AcquirerID">
		<xs:annotation>
			<xs:documentation>Definition: Identification of the Acquirer -- Reference: ISO 8583 - Elem. 32 - acquiring institution identification code -- Usage: Identifications of the Acquirer when the POI System is multi-acquirer.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="DigitString">
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="AcquirerPOIID">
		<xs:annotation>
			<xs:documentation>Definition: Identification of the POI for the payment Acquirer -- Usage: Identification of the POI System or POI Terminal for the Acquirer.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="TextString">
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="AdditionalInformation">
		<xs:annotation>
			<xs:documentation>Definition: Unqualified information.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="TextString">
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="AdditionalInput">
		<xs:annotation>
			<xs:documentation>Definition: Additional information required to verify the PIN like part of the PAN, or driver ID. -- Usage: e.g. the PAN for the PIN Block ISO 1</xs:documentation>
		</xs:annotation>
		<xs:restriction base="TextString">
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="AdditionalProductInfo">
		<xs:annotation>
			<xs:documentation>Definition: Additionl information related to the line item.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="TextString">
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="AdditionalResponse">
		<xs:annotation>
			<xs:documentation>Definition: Additional information related to processing status of a message request . -- Usage: For logging purpose, in order to allow further analysis, statistics and deferred processing on the success or the failure of the request processing.This data element could be send when the message request has been processed successfully</xs:documentation>
		</xs:annotation>
		<xs:restriction base="TextString">
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="AdminRequest">
		<xs:annotation>
			<xs:documentation>Definition: Content of the Custom Admin Request message. -- Usage: Empty</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ServiceIdentification" type="ServiceIdentification" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identification of the administrative service to process.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="AdminResponse">
		<xs:annotation>
			<xs:documentation>Definition: Content of the Custom Admin Response message. -- Usage: It conveys the result of the Custom Admin.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Response" type="Response">
				<xs:annotation>
					<xs:documentation>Result of a message request processing.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="AlignmentTypeCodeZ">
		<xs:union memberTypes="AlignmentType TypeCodeExtension"/>
	</xs:simpleType>
	<xs:simpleType name="AlignmentType">
		<xs:annotation>
			<xs:documentation>Definition: Alignment of the text string on the display line or print line -- Usage: Absence of this data element means the characters have a normal alignment.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Left"/>
			<xs:enumeration value="Right"/>
			<xs:enumeration value="Centred"/>
			<xs:enumeration value="Justified"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="AllowedLoyaltyBrand">
		<xs:annotation>
			<xs:documentation>Definition: Loyalty brands or programs allowed by the Sale System for the loyalty transaction. -- Usage: Restrict the loyalty to some predefined loyalty programs.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="TextString">
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="AllowedPaymentBrand">
		<xs:annotation>
			<xs:documentation>Definition: Card payment brands allowed by the Sale System for the payment transaction. -- Usage: Restrict the payment to some predefined payment cards.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="TextString">
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="AllowedProduct">
		<xs:annotation>
			<xs:documentation>Definition: Product that are payable by the payment card. -- Usage: Restriction of product payable by a card.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ProductLabel" type="ProductLabel" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Product name of an item purchased with the transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AdditionalProductInfo" type="AdditionalProductInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Additionl information related to the line item.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ProductCode" type="ProductCode" use="required">
			<xs:annotation>
				<xs:documentation>Product code of item purchased with the transaction.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="EanUpc" type="EanUpc" >
			<xs:annotation>
				<xs:documentation>Standard product code of item purchased with the transaction.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:simpleType name="AllowedProductCode">
		<xs:annotation>
			<xs:documentation>Definition: Product codes that are payable by the payment card. -- Usage: If not all the products are accepted for the payment card proposed by the Customer. In this case, Result is failure, ErrorCondition is Payment Restriction. For One Time Reservation, the POI can send product codes payable by the payment,</xs:documentation>
		</xs:annotation>
		<xs:restriction base="DigitString">
			<xs:minLength value="1"/>
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="AmountsReq">
		<xs:annotation>
			<xs:documentation>Definition: Various amounts related to the payment and loyalty request from the Sale System. -- Usage: Amounts requested by the Sale System for the payment and loyalty transaction, containing: The currency which is the same for all these amounts The requested amount to pay The cash back part of the requested amount for a payment with cash</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Currency" type="ISOCurrency3A" use="required">
			<xs:annotation>
				<xs:documentation>Currency of a monetary amount.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="RequestedAmount" type="SimpleAmountType" >
			<xs:annotation>
				<xs:documentation>Amount requested by the Sale for the payment. --Rule: Absent if the maximum amount is unknown for a OneTimeReservationThe value has to be greater than 0.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CashBackAmount" type="SimpleAmountType" >
			<xs:annotation>
				<xs:documentation>The cash-back part of the amount requested by the Sale for the payment. --Rule: if payment with cash back. This data has to be performed by the POI. If cash back is not allowed, the payment is</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TipAmount" type="SimpleAmountType" >
			<xs:annotation>
				<xs:documentation>Amount paid for a tip. --Rule: If payment with tip requested by the Sale System.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PaidAmount" type="SimpleAmountType" >
			<xs:annotation>
				<xs:documentation>Amount already paid amount in case of split payment. --Rule: If SplitPaymentFlag is True (split amount of a split payment).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MinimumAmountToDeliver" type="SimpleAmountType" >
			<xs:annotation>
				<xs:documentation>Minimum amount the Sale System is allowed to deliver for this payment. --Rule: if unknown maximum amount for a OneTimeReservation or minimum amount requested by the Sale System</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MaximumCashBackAmount" type="SimpleAmountType" >
			<xs:annotation>
				<xs:documentation>Maximum amount which could be requested for cash-back to the Sale System. --Rule: Maximum amount which could be requested for cash-back to the Sale System.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MinimumSplitAmount" type="SimpleAmountType" >
			<xs:annotation>
				<xs:documentation>Minimum amount of a split, which could be requested by a Customer. --Rule: Minimum amount of a split, which could be requested.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="AmountsResp">
		<xs:annotation>
			<xs:documentation>Definition: Various amounts related to the payment response from the POI System. -- Usage: Amounts approved by the POI and the Acquirer for the payment and loyalty transaction, containing: The authorised amount to be paid The amount of the rebates The amount of financial fees The cash back part of the requested amount for a</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Currency" type="ISOCurrency3A" >
			<xs:annotation>
				<xs:documentation>Currency of a monetary amount. --Rule: Mandatory for currency conversion.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AuthorizedAmount" type="SimpleAmountType" use="required">
			<xs:annotation>
				<xs:documentation>The amount authorized by the Acquirer for the payment transaction.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TotalRebatesAmount" type="SimpleAmountType" >
			<xs:annotation>
				<xs:documentation>Sum of rebates in amount (total amount or line item amount) for all the loyalty programs.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TotalFeesAmount" type="SimpleAmountType" >
			<xs:annotation>
				<xs:documentation>Total amount of financial fees.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CashBackAmount" type="SimpleAmountType" >
			<xs:annotation>
				<xs:documentation>The cash-back part of the amount requested by the Sale for the payment. --Rule: if payment with cash back</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TipAmount" type="SimpleAmountType" >
			<xs:annotation>
				<xs:documentation>Amount paid for a tip. --Rule: If payment with tip requested by the Sale System.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:simpleType name="AmountValue">
		<xs:annotation>
			<xs:documentation>Definition: Value of an amount.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Decimal">
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="APDUClass">
		<xs:annotation>
			<xs:documentation>Definition: Class field of the APDU command (CLA) -- Reference: ISO 7816-4 -- Usage: APDU request for Card Reader device request. For specific card like synchronous card, a private value should be used in accordance to ISO 7816-4 (private range D0-FE)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="ByteSequence">
			<xs:length value="1"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="APDUData">
		<xs:annotation>
			<xs:documentation>Definition: Data field of the APDU command (Lc + Data) -- Reference: ISO 7816-4 -- Usage: APDU request for Card Reader device request. APDU response for Card Reader device response.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="ByteSequence">
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="APDUExpectedLength">
		<xs:annotation>
			<xs:documentation>Definition: Expected length of the data field of the APDU response to the command (Le) -- Reference: ISO 7816-4 -- Usage: APDU request for Card Reader device request.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="ByteSequence">
			<xs:length value="1"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="APDUInstruction">
		<xs:annotation>
			<xs:documentation>Definition: Instruction field of the APDU command (INS) -- Reference: ISO 7816-4 -- Usage: APDU request for Card Reader device request.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="ByteSequence">
			<xs:length value="1"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="APDUPar1">
		<xs:annotation>
			<xs:documentation>Definition: Parameter 1 field of the APDU command (P1) -- Reference: ISO 7816-4 -- Usage: APDU request for Card Reader device request.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="ByteSequence">
			<xs:length value="1"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="APDUPar2">
		<xs:annotation>
			<xs:documentation>Definition: Parameter 2 field of the APDU command(P2) -- Reference: ISO 7816-4 -- Usage: APDU request for Card Reader device request.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="ByteSequence">
			<xs:length value="1"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ApplicationName">
		<xs:annotation>
			<xs:documentation>Definition: Name of the software product. -- Usage: Sent in the Login Request (resp. Response) to identify the Sale System (resp. POI System) product name during the session.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="TextString">
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ApprovalCode">
		<xs:annotation>
			<xs:documentation>Definition: Code assigned to a transaction approval by the Acquirer. -- Reference: ISO 8583 - Elem. 38 - approval code -- Usage: Could be an identifier of the approved transaction for the Acquirer. This data element is conditional for the Loyalty Acquirers. Used in the PaymentRequest request for a referral</xs:documentation>
		</xs:annotation>
		<xs:restriction base="TextString">
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="AreaSize">
		<xs:annotation>
			<xs:documentation>Definition: Size of an area -- Usage: Contain the size of the pad area where the signature is written, given with the maximum abissa and ordinate values (X and Y). The maximum value is "FFFF".</xs:documentation>
		</xs:annotation>
		<xs:attribute name="X" type="X" use="required">
			<xs:annotation>
				<xs:documentation>Abscissa of a point coordinates.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Y" type="Y" use="required">
			<xs:annotation>
				<xs:documentation>Ordinate of a point coordinates.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:simpleType name="ATRValue">
		<xs:annotation>
			<xs:documentation>Definition: Value of the Answer To Reset of a chip card -- Reference: ISO 7816-3 -- Usage: Card reader device response</xs:documentation>
		</xs:annotation>
		<xs:restriction base="ByteSequence">
			<xs:minLength value="1"/>
			<xs:maxLength value="100"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="AuthenticationMethod">
		<xs:list itemType="AuthenticationMethodType"/>
	</xs:simpleType>
	<xs:simpleType name="AuthenticationMethodTypeCodeZ">
		<xs:union memberTypes="AuthenticationMethodType TypeCodeExtension"/>
	</xs:simpleType>
	<xs:simpleType name="AuthenticationMethodType">
		<xs:annotation>
			<xs:documentation>Definition: Method for customer authentication. -- Usage: Allows the Sale System informed about customer authentication for the payment transaction.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Bypass">
				<xs:annotation>
					<xs:documentation>Authentication bypassed by the merchant.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ManualVerification">
				<xs:annotation>
					<xs:documentation>Manual verification, for example passport or drivers license.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MerchantAuthentication">
				<xs:annotation>
					<xs:documentation>Merchant-related authentication.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OfflinePIN">
				<xs:annotation>
					<xs:documentation>Off-line PIN authentication (Personal Identification Number).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OnLinePIN">
				<xs:annotation>
					<xs:documentation>On-line PIN authentication (Personal Identification Number).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PaperSignature">
				<xs:annotation>
					<xs:documentation>Handwritten paper signature.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SecuredChannel">
				<xs:annotation>
					<xs:documentation>Channel-encrypted transaction.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SecureCertificate">
				<xs:annotation>
					<xs:documentation>Secure electronic transaction with cardholder X.509 certificate.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SecureNoCertificate">
				<xs:annotation>
					<xs:documentation>Secure electronic transaction without cardholder certificate.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SignatureCapture">
				<xs:annotation>
					<xs:documentation>Electronic signature capture (handwritten signature).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="UnknownMethod">
				<xs:annotation>
					<xs:documentation>Authentication method is performed unknown.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="AuthorizedAmount">
		<xs:annotation>
			<xs:documentation>Definition: The amount authorized by the Acquirer for the payment transaction. -- Usage: AuthorizedAmount include CashBackAmount na d TipAmount. Payment response from the payment Acquirer. AuthorizedAmount = request.RequestedAmount - TotalRebatesAmount + TotalFeesAmount - (PartialDeclinedAmount)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Decimal">
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="BalanceInquiryRequest">
		<xs:annotation>
			<xs:documentation>Definition: Content of the Balance Inquiry Request message. -- Usage: It conveys Information related to the account for which a Balance Inquiry is requested</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PaymentAccountReq" type="PaymentAccountReq" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Data related to the account pointed by the payment card</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LoyaltyAccountReq" type="LoyaltyAccountReq" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Data related to a requested Loyalty program or account.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="BalanceInquiryResponse">
		<xs:annotation>
			<xs:documentation>Definition: Content of the Balance Inquiry Response message. -- Usage: It conveys the balance and the identification of the associated payment, loyalty or stored value account.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Response" type="Response">
				<xs:annotation>
					<xs:documentation>Result of a message request processing.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentAccountStatus" type="PaymentAccountStatus" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Data related to the result of a Balance Inquiry request. --Rule: If BalanceInquiryRequest. PaymentAccount present</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LoyaltyAccountStatus" type="LoyaltyAccountStatus" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Data related to the result of a loyalty Balance Inquiry. --Rule: If BalanceInquiryRequest. LoyaltyData present</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="BankID">
		<xs:annotation>
			<xs:documentation>Definition: Identification of the bank.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="TextString">
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="BarcodeTypeCodeZ">
		<xs:union memberTypes="BarcodeType TypeCodeExtension"/>
	</xs:simpleType>
	<xs:simpleType name="BarcodeType">
		<xs:annotation>
			<xs:documentation>Definition: Type of BarCode coding. -- Usage: Qualification of the barcode value to display or print</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="EAN8"/>
			<xs:enumeration value="EAN13"/>
			<xs:enumeration value="UPCA"/>
			<xs:enumeration value="Code25"/>
			<xs:enumeration value="Code128"/>
			<xs:enumeration value="PDF417"/>
			<xs:enumeration value="QRCODE"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="BarcodeValue">
		<xs:annotation>
			<xs:documentation>Definition: Value with a BarCode coding. -- Usage: The barcode value to display or print</xs:documentation>
		</xs:annotation>
		<xs:restriction base="TextString">
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="BatchRequest">
		<xs:annotation>
			<xs:documentation>Definition: Content of the Batch Request message. -- Usage: Message to send Payment, Loyalty and Reversal transactions to be performed without the Sale System, or to request the Payment, Loyalty and Reversal transactions that has been performed without the Sale System.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TransactionToPerform" type="TransactionToPerform" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Content of the Batch Request message</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="RemoveAllFlag" type="RemoveAllFlag" >
			<xs:annotation>
				<xs:documentation>A flag requesting to remove the transactions stored and not yet performed. default False.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="BatchResponse">
		<xs:annotation>
			<xs:documentation>Definition: Content of the Batch Response message. -- Usage: It conveys either the response of transactions to perform sent in the batch request, or a collection of results of performed Payment, Loyalty and Reversal transactions in a batch message or file.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Response" type="Response">
				<xs:annotation>
					<xs:documentation>Result of a message request processing.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PerformedTransaction" type="PerformedTransaction" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Result of performed transactions.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="BeepKeyFlag">
		<xs:annotation>
			<xs:documentation>Definition: Indicates, when the user press a key, if a beep has to be generated (value True). -- Usage: During the processing of an Input command.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:boolean">
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="CapturedSignature">
		<xs:annotation>
			<xs:documentation>Definition: Numeric value of a handwritten signature. -- Usage: Contain the value of a handwritten signature, e.g. the signature of a cardholder on the merchant payment receipt. Only one format of the signature is allowed, it contains:  The size of the pad area where the signature is written, given</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AreaSize" type="AreaSize" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Size of an area</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SignaturePoint" type="SignaturePoint" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Coordinates of a point where the pen changes direction or lift.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CardAcquisitionRequest">
		<xs:annotation>
			<xs:documentation>Definition: Content of the Card Acquisition Request message. -- Usage: It conveys Information related to the payment and loyalty cards to read and analyse. This message pair is usually followed by a message pair (e.g. payment or loyalty) which refers to this Card Acquisition message pair. The Card Acquisition</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="SaleData" type="SaleData">
				<xs:annotation>
					<xs:documentation>Data related to the Sale System.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CardAcquisitionTransaction" type="CardAcquisitionTransaction">
				<xs:annotation>
					<xs:documentation>Data related to the payment and loyalty card acquisition.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CardAcquisitionResponse">
		<xs:annotation>
			<xs:documentation>Definition: Content of the Card Acquisition Response message. -- Usage: It conveys Information related to the payment and loyalty cards read and processed by the POI System and entered by the Customer:As for the Payment request, the result of the CardAcquisition and the identification of the transaction.One</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Response" type="Response">
				<xs:annotation>
					<xs:documentation>Result of a message request processing.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SaleData" type="SaleData">
				<xs:annotation>
					<xs:documentation>Data related to the Sale System.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="POIData" type="POIData">
				<xs:annotation>
					<xs:documentation>Data related to the POI System.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentBrand" type="PaymentBrand" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Type of payment card --Rule: Brands available for payment by the card and not chosen by the Customer</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentInstrumentData" type="PaymentInstrumentData" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Data related to the instrument of payment for the transaction. --Rule: If this type of payment card is configured to send information if the CardAcquisition response</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LoyaltyAccount" type="LoyaltyAccount" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Data related to a loyalty account processed in the transaction. --Rule: If loyalty card selected by the customer</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CustomerOrder" type="CustomerOrder" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Customer order attached to a card, recorded in the POI system. --Rule: If the list of customer orders has been requested.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CardAcquisitionTransaction">
		<xs:annotation>
			<xs:documentation>Definition: Data related to the payment and loyalty card acquisition. -- Usage: Elements requested by the Sale System that are global to the payment or loyalty transaction.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AllowedPaymentBrand" type="AllowedPaymentBrand" minOccurs="0" maxOccurs="unbounded">
