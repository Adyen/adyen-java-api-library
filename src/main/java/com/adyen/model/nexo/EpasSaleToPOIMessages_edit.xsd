		<xs:annotation>
			<xs:documentation>Definition: Card track format</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="ISO">
				<xs:annotation>
					<xs:documentation>ISO card track format - ISO 7813 - ISO 4909</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="JIS-I">
				<xs:annotation>
					<xs:documentation>Japenese track format I</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="JIS-II">
