					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="TrackFormat" type="TrackFormatType"  default="ISO">
					<xs:annotation>
						<xs:documentation>Card track format</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
