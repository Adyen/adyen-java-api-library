					<xs:documentation>Card payment brands allowed by the Sale System for the payment transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AllowedLoyaltyBrand" type="AllowedLoyaltyBrand" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Loyalty brands or programs allowed by the Sale System for the loyalty transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ForceEntryMode" type="ForceEntryMode" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Payment instrument entry mode requested by the Sale System.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="LoyaltyHandling" type="LoyaltyHandlingType"  default="Allowed">
			<xs:annotation>
				<xs:documentation>Type of Loyalty processing requested by the Sale System.</xs:documentation>
			</xs:annotation>
